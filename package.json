    "eslint": "9.9.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-promise": "7.1.0",
    "prettier": "3.3.3",
    "run-z": "2.0.0",
    "tsx": "4.19.0",
    "typescript": "5.4.5"
