{
  "name": "discord-event-notifier",
  "version": "1.0.0",
  "description": "When a Discord event is started, send a Mention to the users you have marked as interested.",
  "main": "dist/index.js",
  "repository": "git@github.com:jaoafa/discord-event-notifier.git",
  "author": "Tomachi",
  "private": true,
  "scripts": {
    "build": "ts-node ./src/main.ts",
    "dev": "ts-node-dev ./src/main.ts",
    "start": "node ./dist/main.js",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@types/config": "0.0.41",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "axios": "0.27.2",
    "config": "3.3.7",
    "discord.js": "13.8.1",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "prettier": "2.7.1",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4",
    "webpack-node-externals": "3.0.0",
    "yarn-run-all": "3.1.1"
  }
}
